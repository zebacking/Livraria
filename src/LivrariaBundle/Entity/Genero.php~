<?php

namespace LivrariaBundle\Entity;
use Doctrine\ORM\Mapping as ORM;

/**
 * Description of Genero
 *
 * @author aluno
 * @ORM\Entity
 * @ORM\Table(name="generos")
 */
class Genero
{
    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;
    
    /**
     * @ORM\Column(type="string", length=100)
     */
    private $nome;
    
    
}
